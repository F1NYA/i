				&lt;/p&gt;
			[pattern/mail/new_design/_common_content_end.html]
			[pattern/mail/new_design/_common_feedback.html]   	 
			[pattern/mail/new_design/_common_signature_start.html]
				${sNameOrgan},&lt;br&gt;
			[pattern/mail/new_design/_common_signature_end.html]
			[pattern/mail/new_design/_common_footer.html]
          </activiti:expression>
        </activiti:field>
        <activiti:field name="sPhone_SMS">
          <activiti:expression>${phone}</activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression>Vashe zvernennya [sID_Order] opratsovano, otsinit yakist nadannya posluhy (formu nadislano v email). Detalno igov.org.ua/journal</activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression>" "</activiti:expression>
        </activiti:field>
      </extensionElements>
